AA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA,CADA,CACA;AACA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,OALA;AAMA;AARA,GAvBA;AAkCA,EAAA,QAAA,EAAA;AAAA;AACA,IAAA,IADA,kBACA;AAAA;AACA,aAAA,KAAA,UAAA,CAAA,MAAA;AACA,KAHA;AAIA,IAAA,QAJA,sBAIA;AAAA;;AAAA;AACA,UAAA,QAAA,GAAA,KAAA,UAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AAAA;AACA,eAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAAA,OAAA,EAAA,KAAA,CAAA,UAAA,GAAA;AAAA,iBACA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA,CAAA,CAAA,QAAA,CAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,CADA;AAAA,SAAA,CAAA;AAGA,OAJA,CAAA;AAKA,aAAA,QAAA,CAAA,MAAA,GAAA,CAAA,GAAA,QAAA,GAAA,CAAA;AAAA;AACA,QAAA,EAAA,EAAA,EADA;AAEA,QAAA,QAAA,EAAA,EAFA;AAGA,QAAA,KAAA,EAAA,EAHA;AAIA,QAAA,IAAA,EAAA,EAJA;AAKA,QAAA,GAAA,EAAA,EALA;AAMA,QAAA,GAAA,EAAA,EANA;AAOA,QAAA,QAAA,EAAA,EAPA;AAQA,QAAA,KAAA,EAAA,EARA;AASA,QAAA,MAAA,EAAA,EATA;AAUA,QAAA,IAAA,EAAA,EAVA;AAWA,QAAA,KAAA,EAAA,EAXA;AAYA,QAAA,QAAA,EAAA;AAZA,OAAA,CAAA;AAgBA;AA1BA;AAlCA,CAAA","sourcesContent":["<template>\n\n  <div>\n\n  <h3>Passengers</h3>\n\n<!--Pagination bar-->\n  <b-pagination\n      align=\"center\"\n      v-model=\"currentPage\"\n      :total-rows=\"rows\"\n      :per-page=\"perPage\"\n      aria-controls=\"results-table\"\n    ></b-pagination>\n\n<!--Items per page menu-->\n<div class=\"perPage\">\n<label>Results per page:</label>\n<select v-model=\"perPage\">\n  <option v-for=\"pages in pageLimit \" :key=\"pages\">\n    {{pages}}\n  </option>\n  <option :value=\"passengers.length\">\n    All\n  </option>\n</select>\n</div>\n\n<!--Results table-->\n  <b-table\n      id=\"results-table\"\n      :items=\"filtered\"\n      :per-page=\"perPage\"\n      :current-page=\"currentPage\"\n      small>\n\n      <template slot=\"top-row\" slot-scope=\"{fields}\">\n        <td v-for=\"field in fields\" :key=\"field.key\">\n          <input class=\"col-sm\" v-model=\"filters[field.key]\" :placeholder=\"field.label\">\n        </td>\n      </template>\n\n    </b-table>\n\n  </div>\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport axios from \"axios\";\nimport VueAxios from \"vue-axios\";\nVue.use(VueAxios, axios);\nexport default {\n  name: 'Passengers',\n  mounted(){\n    this.getData();\n  },\n  data(){\n    return{\n      currentPage: 1, //Starting page\n      perPage: 20, //Default of 20 passengers per page\n      pageLimit: 50, //Limitation to results per page\n      apiURL: \"http://coding-task.strakertranslations.com/passengers\",//API URL\n      passengers: [], //empty Array or api data\n      filters: {  //When the filter input is typed in, filters will equal to the field name of that input. eg...Name: \"input...\"\n        // Name: \"\",\n        // Age: \"\",\n        // Ticket: \"\",\n        // Embarked: \"\",\n        // class: \"\"\n        //TODO Wanted to narrow down on filtering capabilities to these specific groups\n      }\n    }\n  },\n\n  methods: {\n    getData(){\n      axios.get(this.apiURL) //fetch data from this URL\n           .then((response) =>{\n             this.passengers = response.data; //make passengers array list equal to api response data\n             // console.log(response);\n             console.log(this.filters);\n           })\n    }\n  },\n\n  computed:{ //Computed Properties\n    rows(){ //Number of rows\n      return this.passengers.length;\n    },\n    filtered(){ //search filter functionality\n      const filtered = this.passengers.filter(item => { //filter passengers data into new array\n        return Object.keys(this.filters).every(key =>\n          String(item[key]).includes(this.filters[key]));\n\n      })\n      return filtered.length > 0 ? filtered :[{ //if the filtered data is greater than 0 then use filtered, else empty the input values\n        id:\"\",\n        Survived:\"\",\n        class: \"\",\n        Name: \"\",\n        Sex: \"\",\n        Age: \"\",\n        Siblings:\"\",\n        Parch: \"\",\n        Ticket: \"\",\n        Fare: \"\",\n        Cabin: \"\",\n        Embarked: \"\",\n\n      }]\n\n    }\n  },\n}\n</script>\n\n<!-- CSS confined to this component -->\n<style scoped>\n\nh3 {\n  margin: 30px;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\n\np{\n  margin: 20px;\n}\n\nselect{\n  margin-left: 10px;\n}\n\n.perPage {\n  float:left;\n  margin: 20px;\n}\n\n.title{\n  font-weight: bold;\n}\n\n.perPage {\n  float:left;\n  margin: 10px;\n}\n\n</style>\n"],"sourceRoot":"src/components"}]}